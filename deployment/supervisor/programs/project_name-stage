

[program:project_name-stage]

environment=DJANGO_SETTINGS_MODULE=project.settings._project_name_stage

directory=%(ENV_HOME)s/sites/project_name-stage
command=
    %(ENV_HOME)s/sites/project_name-stage/virtualenv/bin/gunicorn
    --bind unix:///%(ENV_HOME)s/sites/project_name-stage.sock
    --worker-class gevent
    --workers 2
    project.wsgi

autostart=true
autorestart=true

stdout_logfile=%(ENV_HOME)s/supervisor/logs/%(program_name)s.log
stdout_logfile_maxbytes=10MB
stderr_logfile=%(ENV_HOME)s/supervisor/logs/%(program_name)s.err
stderr_logfile_maxbytes=10MB
